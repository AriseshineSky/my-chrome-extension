{
	"manifest_version": 3,
	"name": "React Chrome Extension",
	"version": "1.0.0",
	"description": "amazon order collect",
	"action": {
		"default_popup": "index.html"
	},
	"background": {
		"service_worker": "background.js"
	},
	"permissions": [
		"tabs",
		"storage",
		"activeTab"
	],
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"content.js"
			]
		}
	],
	"host_permissions": [
		"http://*/*",
		"https://*/*"
	]
}
