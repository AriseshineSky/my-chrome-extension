{
	"manifest_version": 3,
	"name": "React Chrome Extension",
	"version": "1.0.0",
	"description": "amazon order collect",
	"action": {
		"default_popup": "index.html"
	},
	"background": {
		"service_worker": "background/background.js"
	},
	"permissions": [],
	"content_scripts": [
		{
			"matches": ["https://www.amazon.com/*"],
			"js": ["content/content.js"]
		}
	],
	"host_permissions": ["http://*/*", "https://*/*"]
}
